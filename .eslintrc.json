{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:html/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "settings": {
    "html/html-extensions": [".html"]
  }
}
