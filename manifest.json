                            {
                                "name": "RibbonApi",
                                "minVersion": "1.1"
                            }
                        ],
                        "formFactors": [
                            "desktop"
                        ]
                    },
                    "id": "runtime_1",
                    "type": "general",
                    "code": {
                        "page": "https://localhost:3000/commands.html",
                        "script": "https://localhost:3000/commands.bundle.js"
                    },
                    "lifetime": "short",
                    "actions": [
                        {
                            "id": "disableZip",
                            "type": "executeFunction",
                            "displayName": "disableZip",
                            "multiselect": false,
                            "supportsNoItemContext": false
                        },
                        {
                            "id": "enableZip",
                            "type": "executeFunction",
                            "displayName": "enableZip",
                            "multiselect": false,
                            "supportsNoItemContext": false
