        "privacyUrl": "https://github.com/olahaye74/ZipMail",
        "termsOfUseUrl": "https://github.com/olahaye74/ZipMail"
    },
    "name": {
        "short": "ZipMail",
        "full": "ZipMail"
    },
    "description": {
        "short": "Compresse les messages en ZIP",
        "full": "Compresse les messages en ZIP"
    },
    "icons": {
        "outline": "assets/icon-zip-64.png",
        "color": "assets/icon-zip-128.png"
    },
    "accentColor": "#230201",
    "validDomains": [
        "https://localhost:3000"
    ],
    "showLoadingIndicator": false,
    "isFullScreen": false,
    "defaultBlockUntilAdminAction": false,
    "authorization": {
        "permissions": {
            "resourceSpecific": [
                {
                    "name": "MailboxItem.ReadWrite.User",
                    "type": "Delegated"
                }
            ]
        }
    },
