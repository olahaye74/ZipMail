                        {
                            "id": "enableZipEncrypted",
                            "type": "executeFunction",
                            "displayName": "enableZipEncrypted",
                            "multiselect": false,
                            "supportsNoItemContext": false
                        },
                        {
                            "id": "onMessageSend",
                            "type": "executeFunction",
                            "displayName": "onMessageSend",
                            "multiselect": false,
                            "supportsNoItemContext": false
